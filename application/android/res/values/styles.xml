<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AnimationActivity" parent="@android:style/Animation.Activity" mce_bogus="1">    
		<item name="android:activityOpenEnterAnimation">@anim/showani</item>          
		<item name="android:activityOpenExitAnimation">@anim/hideani</item>          
		<item name="android:activityCloseEnterAnimation">@anim/showani</item>          
		<item name="android:activityCloseExitAnimation">@anim/hideani</item>      
	</style>
	     <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
	<style name="Theme.UPPay" parent="android:style/Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>
